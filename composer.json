{
    "name": "jlourenco/base",
    "type": "library",
    "description": "This is the basic module for my ready to deploy website structure",
    "keywords": [
        "jlourenco",
        "base"
    ],
    "homepage": "https://github.com/jlourenco/base",
    "license": "MIT",
    "authors": [
        {
            "name": "Joao Lourenco",
            "email": "jglourenco.pt@gmail.com",
            "homepage": "http://joaolourenco.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "jlourenco/support" : "0.1.*",
        "yajra/laravel-datatables-oracle": "^6.3",
        "cartalyst/sentinel": "^2.0",
        "tom-lingham/searchy" : "2.*",
        "nicolaslopezj/searchable": "^1.7",
        "jenssegers/agent": "^2.3",
        "torann/geoip": "^0.2.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "jlourenco\\base\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jlourenco\\base\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
